{
  "name": "fairbank-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar --include-path node_modules --output-style compressed --source-map true --source-map-contents src/assets/scss -o src/assets/css",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path node_modules --output-style compressed --source-map true --source-map-contents src/assets/scss -o src/assets/css -w",
    "start-js": "react-scripts-ts start",
    "build-js": "react-scripts-ts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts-ts test --env=jsdom",
    "coverage": "react-scripts-ts test --env=jsdom --coverage",
    "eject": "react-scripts-ts eject"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.0",
    "card-validator": "^4.3.0",
    "chart.js": "^2.7.2",
    "crypto-js": "^3.1.9-1",
    "immutable": "^3.8.2",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "qrcode.react": "^0.8.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.1.0",
    "@types/card-validator": "^4.1.0",
    "@types/chart.js": "^2.7.14",
    "@types/crypto-js": "^3.1.39",
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.2.3",
    "@types/jquery": "^3.3.1",
    "@types/node": "^9.6.6",
    "@types/qrcode.react": "^0.6.3",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.16",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-router-redux": "^5.0.13",
    "@types/stripe-v3": "^3.0.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "fsevents": "^1.2.0",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "react-scripts-ts": "2.15.1",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.3"
  }
}
