{
  "name": "fairbank-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar --include-path node_modules --output-style compressed --source-map true --source-map-contents src/assets/scss -o src/assets/css",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path node_modules --output-style compressed --source-map true --source-map-contents src/assets/scss -o src/assets/css -w",
    "start-js": "react-scripts-ts start",
    "build-js": "react-scripts-ts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts-ts test --env=jsdom",
    "coverage": "react-scripts-ts test --env=jsdom --coverage",
    "eject": "react-scripts-ts eject"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "^4.0.0",
    "chart.js": "^2.7.1",
    "immutable": "^3.8.2",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.1",
    "qrcode.react": "^0.7.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-popper": "^0.8.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@types/chart.js": "^2.7.3",
    "@types/enzyme": "^3.1.6",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/jest": "^22.0.1",
    "@types/jquery": "^3.3.1",
    "@types/node": "^9.3.0",
    "@types/qrcode.react": "^0.6.2",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-router-redux": "^5.0.11",
    "babel-eslint": "^8.2.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "fsevents": "^1.1.3",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "react-scripts-ts": "2.10.0"
  }
}
